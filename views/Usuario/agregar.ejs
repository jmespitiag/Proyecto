<%- include("../header"); %>
<div class="col-12">
    <h1>Agregar Usuario</h1>
    <form method="post" action="/Usuario/insertar">
        <div class="form-group">
            <label for="id_nodo">id_nodo</label>
            <input required id="id_nodo" placeholder="Id del Usuario" class="form-control" type="number"
                name="id_nodo">
        </div>
        <div class="form-group">
            <label for="ref_bancaria">ref_bancaria</label>
            <input required id="ref_bancaria" placeholder="Referencia bancaria del Usuario" class="form-control" type="number"
                name="ref_bancaria">
        </div>
        <div class="form-group">
            <label for="email">email</label>
            <input required id="email" placeholder="Email del Usuario" class="form-control" type="text"
                name="email">
        </div>
        <div class="form-group">
            <label for="genero">genero</label>
            <input required id="genero" placeholder="Genero del Usuario" class="form-control" type="text"
                name="genero">
        </div>
        <div class="form-group">
            <label for="nombre">nombre</label>
            <input required id="nombre" placeholder="Nombre del Usuario" class="form-control" type="text"
                name="nombre">
        </div>
        <div class="form-group">
            <label for="doc_identidad">doc_identidad</label>
            <input required id="doc_identidad" placeholder="Documento de identidad del Usuario" class="form-control" type="number"
                name="doc_identidad">
        </div>
        
        <div class="form-group">
            <button class="btn btn-success">Guardar</button>
            &nbsp;<a href="/Usuario" class="btn btn-primary">Volver</a>
        
    </form>
</div>
<%- include("../footer"); %>