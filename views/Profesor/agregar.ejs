<%- include("../header"); %>
<div class="col-12">
    <h1>Agregar Profesor</h1>
    <form method="post" action="/Profesor/insertar">
        <div class="form-group">
            <label for="doc_id">doc_id</label>
            <input required id="doc_id" placeholder="Id del profesor" class="form-control" type="number"
                name="doc_id">
        </div>
        <div class="form-group">
            <label for="nombre">nombre</label>
            <input required id="nombre" placeholder="Nombre del Profesor" class="form-control" type="text"
                name="nombre">
        </div>
        <div class="form-group">
            <label for="email">email</label>
            <input required id="email" placeholder="Email del profesor" class="form-control" type="text"
                name="email">
        </div>
        <div class="form-group">
            <label for="telefono">telefono</label>
            <input required id="telefono" placeholder="Telefono del profesor" class="form-control" type="number"
                name="telefono">
        </div>
        <div class="form-group">
            <label for="area_p">area_p</label>
            <input required id="area_p" placeholder="Area principal del profesor" class="form-control" type="text"
                name="area_p">
        </div>
        <div class="form-group">
            <label for="area_a">area_a</label>
            <input required id="area_a" placeholder="Area alternativa del profesor" class="form-control" type="text"
                name="area_a">
        </div>
        <div class="form-group">
            <label for="contraseña">contraseña</label>
            <input required id="contraseña" placeholder="Contraseña del profesor" class="form-control" type="text"
                name="contraseña">
        </div>
        
        <div class="form-group">
            <button class="btn btn-success">Guardar</button>
            &nbsp;<a href="/Profesor" class="btn btn-primary">Volver</a>
        
    </form>
</div>
<%- include("../footer"); %>